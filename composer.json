{
  "name": "QACorp/QTeste-QAraCT",
  "description": "Módulo do QTeste para gerar casos de testes via ChatGPT",
  "keywords": ["laravel","php", "openai", "sdk", "codex", "GPT-3", "DALL-E", "api", "client", "natural", "language", "processing"],
  "license": "MIT",
  "version": "v0.0.8",
  "authors": [
    {
      "name": "Marco Aurélio Lima Fernandes",
      "email": "malf88@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.0",
    "spatie/laravel-data": "^3.2",
    "spatie/laravel-permission": "^5.10",
    "openai-php/laravel": "^0.9.1"

  },
  "autoload": {
    "psr-4": {
      "App\\Modules\\QAraCasosTeste\\": "QAraCasosTeste/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "Tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "App\\Modules\\QAraCasosTeste\\Providers\\QAraServiceProvider"
      ]
    }
  }
}
